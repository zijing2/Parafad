<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
			<resource id="Magento_Backend::admin">
				<resource id="MGS_Mmegamenu::mmegamenu" title="Mmegamenu" sortOrder="100">
					<resource id="MGS_Mmegamenu::save" title="Save Item" sortOrder="10" />
					<resource id="MGS_Mmegamenu::delete" title="Delete Item" sortOrder="20" />
                </resource>
				
				<resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MGS_Mmegamenu::config_megamenu" title="Megamenu Setting" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
