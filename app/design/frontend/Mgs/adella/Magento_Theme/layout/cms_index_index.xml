<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<link rel="stylesheet" type="text/css" src='https://fonts.googleapis.com/css?family=Quicksand:400,700' src_type="url" />
	</head>
	<referenceBlock name="breadcrumbs" remove="true"/>
	<referenceContainer name="page.wrapper">
		<block class="Magento\Framework\View\Element\Template" name="newletter-popup" template="Magento_Theme::newletter-popup.phtml" after="footer-container"/>
		<block class="Magento\Framework\View\Element\Template" name="script" template="Magento_Theme::script.phtml" after="footer-container"/>
    </referenceContainer>
</page>
